{
  "description": "Exact replication of Hikima et al. paper setup for validation",
  "data": {
    "s3_bucket": "rideshare-benchmark-data",
    "area_info_key": "reference/area_info.csv",
    "base_url": "https://d37ci6vzurychx.cloudfront.net/trip-data",
    "cache_dir": "./data/cache",
    "min_trip_distance": 0.001,
    "min_total_amount": 0.001,
    "max_location_id": 264
  },
  "algorithms": {
    "hikima_alpha": 18.0,
    "hikima_taxi_speed": 25.0,
    "hikima_base_price": 5.875,
    "hikima_epsilon": 1e-10,
    "maps_price_discretization": 0.05,
    "maps_max_distance": 2.0,
    "maps_s0_rate": 1.5,
    "linucb_alpha": 0.5,
    "linucb_arm_multipliers": [0.6, 0.8, 1.0, 1.2, 1.4],
    "lp_solver": "CBC",
    "lp_time_limit": 300
  },
  "acceptance": {
    "pl_alpha": 1.5,
    "sigmoid_beta": 1.3,
    "sigmoid_gamma": 0.17
  },
  "experiment": {
    "vehicle_type": "green",
    "year": 2019,
    "month": 10,
    "borough": "Manhattan",
    "start_hour": 10,
    "end_hour": 20,
    "time_interval_minutes": 5,
    "simulation_range": 120,
    "num_evaluations": 100,
    "methods": ["hikima", "maps", "linucb"],
    "acceptance_function": "PL",
    "start_day": 3,
    "end_day": 4,
    "notes": {
      "simulation_range_explanation": "120 scenarios = 10 hours * 12 (5-minute intervals per hour)",
      "time_setup": "10:00-20:00 business hours as in original paper",
      "days_setup": "Days 3-4 of October 2019 as in original experiments",
      "expected_performance": {
        "hikima": "~1250 avg objective value",
        "maps": "~1180 avg objective value", 
        "linucb": "~1145 avg objective value"
      }
    }
  }
} 