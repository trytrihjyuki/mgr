{
  "experiment_name": "hikima_replication_experiment",
  "experiment_description": "Exact replication of Hikima et al. experimental setup",
  "experiment_version": "1.0.0",
  "data_config": {
    "s3_bucket": "taxi-pricing-benchmark",
    "data_source_base_url": "https://d37ci6vzurychx.cloudfront.net/trip-data",
    "vehicle_types": [
      "green",
      "yellow",
      "fhv"
    ],
    "location_boundaries": {
      "Manhattan": {
        "lat_min": 40.7,
        "lat_max": 40.8,
        "lon_min": -74.02,
        "lon_max": -73.93
      },
      "Brooklyn": {
        "lat_min": 40.57,
        "lat_max": 40.74,
        "lon_min": -74.04,
        "lon_max": -73.83
      },
      "Queens": {
        "lat_min": 40.54,
        "lat_max": 40.8,
        "lon_min": -73.96,
        "lon_max": -73.7
      },
      "Bronx": {
        "lat_min": 40.79,
        "lat_max": 40.92,
        "lon_min": -73.93,
        "lon_max": -73.77
      },
      "Staten Island": {
        "lat_min": 40.477,
        "lat_max": 40.651,
        "lon_min": -74.26,
        "lon_max": -74.05
      }
    },
    "taxi_zones_file": "reference_data/taxi_zones.csv",
    "min_trip_distance": 0.001,
    "min_total_amount": 0.001,
    "distance_conversion_factor": 1.60934
  },
  "time_config": {
    "start_hour": 10,
    "end_hour": 20,
    "time_window_minutes": 5,
    "start_date": "2019-10-01",
    "end_date": "2019-10-02",
    "simulation_scenarios": 24,
    "monte_carlo_iterations": 100
  },
  "hikima_config": {
    "method_name": "HikimaMinMaxCostFlow",
    "alpha": 18.0,
    "s_taxi": 25.0,
    "epsilon": 1e-10,
    "initial_delta_factor": 1.0,
    "delta_reduction_factor": 0.5,
    "min_delta": 0.001,
    "piecewise_linear_params": {
      "c_factor": 2.0,
      "d": 3.0
    },
    "sigmoid_params": {
      "beta": 1.3,
      "gamma": 0.16539880833293433
    }
  },
  "maps_config": {
    "method_name": "MAPS",
    "alpha": 18.0,
    "s_taxi": 25.0,
    "s_0_rate": 1.5,
    "price_discretization_rate": 0.05,
    "max_matching_distance_km": 2.0
  },
  "linucb_config": {
    "method_name": "LinUCB",
    "ucb_alpha": 0.5,
    "base_price": 5.875,
    "price_multipliers": [
      0.6,
      0.8,
      1.0,
      1.2,
      1.4
    ],
    "use_time_features": true,
    "use_location_features": true,
    "use_distance_features": true
  },
  "lp_config": {
    "method_name": "LinearProgram",
    "min_price_factor": 0.5,
    "max_price_factor": 2.0,
    "price_grid_size": 10,
    "solver_name": "PULP_CBC_CMD",
    "solver_timeout": 300,
    "solver_verbose": false,
    "acceptance_model": "sigmoid"
  },
  "aws_config": {
    "s3_bucket": "taxi-pricing-benchmark",
    "s3_results_prefix": "experiments/results",
    "s3_data_prefix": "datasets",
    "s3_reference_prefix": "reference_data",
    "lambda_timeout": 900,
    "lambda_memory": 3008,
    "aws_region": "us-east-1"
  },
  "methods_to_run": [
    "HikimaMinMaxCostFlow",
    "MAPS",
    "LinUCB",
    "LinearProgram"
  ],
  "enable_statistical_testing": true,
  "significance_level": 0.05
}